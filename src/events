class Jonathan extends React.Component{

    saludaConLaMano(){
        console.log("saludando con la mano")
    }

    render(){
        return (
            <div>
                <h1>Button 1</h1>
                <button onClick={this.saludaConLaMano}> push</button>
                <h1>Button 2</h1>
                <button onClick={this.saludaConLaMano}>press</button>
            </div>
        )
    }
}
  

ReactDOM.render(
    <Jonathan/>,
    document.getElementById('root')
)